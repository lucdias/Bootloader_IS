#include <stdio.h>
#include <stdlib.h>

int main(){
    FILE *cop;
    int w,j,k,l,i,cont=0;
    int r,d;
    int matriz[240][1000]
    cop=fopen("test.bmp", "rb");

    if(cop!=NULL){
        for(k=0;k<54;k++){
            fgetc(cop);
        }
        for(k=0;k<240;k++){
            for(cont =0;cont < 1000;cont ++){   
                d=fgetc(cop);
                matriz[k] [cont] = d;
            }
        }
    }  
    fclose(cop);
	return 0;
}